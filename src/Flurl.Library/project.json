{
  "title": "Flurl",
  "version": "2.1.0-beta3-update1",

  "buildOptions": {
    "compile": {
      "include": "../Flurl.Shared/**/*.cs"
    },
    "optimize": true,
    "outputName": "Flurl",
    "xmlDoc": true
  },

  "frameworks": {
    "portable40-net40+win8+wpa81": {
      "frameworkAssemblies": {
        "mscorlib": "2.0.5.0",
        "System": "2.0.5.0",
        "System.Core": "2.0.5.0"
      }
    },
    "netstandard1.3": {
      "buildOptions": {
        "define": [ "NETSTD" ]
      },
      "dependencies": {
        "System.Globalization": "4.0.11-rc2-24027",
        "System.Linq": "4.1.0-rc2-24027",
        "System.Reflection.TypeExtensions": "4.1.0-rc2-24027"
      }
    }
  }
}